<!--ono/signin/signin.wxml-->
<view class="containers">
  <view class="yearmonth">
    <view>{{yearMonth}}</view>
  </view>
  <view>
  
  
  
  
  </view>
</view>

<view class="sign-detail">
  <view class="sign-canvas">
      <canvas class="cir" canvas-id="canvasArc1"></canvas> 
      <text class="cc">10/31</text> 
      <text class="cr">本周完成数</text>
  </view>

  <view class="sign-canvas">
      <canvas class="cir" canvas-id="canvasArc2"></canvas> 
      <text class="cc">10/31</text> 
      <text class="cr">本月完成数</text>
  </view>

    <view class="sign-canvas">
      <canvas class="cir" canvas-id="canvasArc3"></canvas> 
      <text class="cc">10/31</text> 
      <text class="cr">本年完成数</text>
  </view>
 </view>


<swiper  class="sign-add">

  <swiper-item>
    <button type="primary" catchtap='makesignin' data-status='open'>完成今日打卡</button>
  </swiper-item>
  <swiper-item style="width: 375px;">
    <button type="warn" data-status='open' catchtap='signDelete' data-id='{{id}}'>删除</button>
  </swiper-item>
</swiper>

